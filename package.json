    "pm2:logs": "pm2 logs ecosystem.config.js",
    "deploy:prod": "npm run build && npm run start:prod",
    "lint": "vue-tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "setup:env": "cp .env.example .env.local",
